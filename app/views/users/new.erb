<h2>Users</h2>

<%if flash[:message]%>
  <i><h3><%=flash[:message]%></h3></i>
<% end %>

<h3>Sign Up</h3>
<form action="/signup" method="POST">
  <p>Username: <input type="text" name="username"></p>
  <p>Password: <input type="text" name="password"></p>
  <input type="submit" value="submit">
</form>
